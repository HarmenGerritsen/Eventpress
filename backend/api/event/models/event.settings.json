{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titel": {
      "type": "string",
      "required": true
    },
    "Datum": {
      "type": "date",
      "required": true
    },
    "Tijd": {
      "type": "time",
      "required": true
    },
    "Omschrijving": {
      "type": "text"
    },
    "Organisator": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "Categorie": {
      "type": "enumeration",
      "enum": [
        "frontend",
        "backend",
        "overig"
      ]
    },
    "Locatie": {
      "type": "string",
      "required": true
    }
  }
}
